{
    "globalSetup": "<rootDir>/src/.jest/globalSetup.ts",
    "globalTeardown": "<rootDir>/src/.jest/globalTearDown.ts",
    "moduleFileExtensions": ["js", "json", "ts"],
    "preset": "ts-jest",
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".ispec.ts",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "setupFiles": [
      "<rootDir>/src/.jest/setupEnvVars.ts"
    ]
  }