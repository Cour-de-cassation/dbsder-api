              "schema": { "$ref": "#/components/schemas/UpdateDecisionPseudonymiseeDTO" }
            }
          }
        },
        "responses": {
          "204": { "description": "" },
          "400": { "description": "Statut manquant ou invalide" },
          "401": { "description": "Vous n'êtes pas autorisé à appeler cette route" },
          "404": { "description": "La decision n'a pas été trouvée" }
        },
        "tags": ["DbSder"]
      }
    },
