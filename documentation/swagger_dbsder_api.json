            "items": { "type": "string" }
          },
          "formation": {
            "type": "string",
            "description": "Propriété interne nécessaire à la publication des décisions",
            "example": "someFormation"
          },
          "blocOccultation": {
            "type": "number",
            "description": "Bloc d'occultation à respecter pour la décision",
