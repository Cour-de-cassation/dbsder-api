  "components": {
    "schemas": {
      "GetDecisionByIdResponse": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "description": "Identifiant de la décision",
            "example": "507f1f77bcf86cd799439011"
          },
          "analysis": {
            "type": "object",
            "description": "Analyse de la décision",
            "example": {
              "analyse": ["someAnalyse"],
              "doctrine": "someDoctrine",
              "link": "someLink",
              "reference": ["someReference"],
              "source": "someSource",
              "summary": "someSummary",
              "target": "someTarget",
              "title": ["someTitle"]
            }
          },
          "appeals": {
            "description": "Liste des pourvois",
            "example": ["someAppeal"],
            "type": "array",
            "items": { "type": "string" }
          },
          "chamberId": {
            "type": "string",
            "description": "Identifiant de la chambre",
            "example": "someChamberId"
          },
          "chamberName": {
            "type": "string",
            "description": "Nom de la chambre",
            "example": "someChamberName"
          },
          "dateCreation": {
            "type": "string",
            "description": "Date de création de la décision au format ISO 8601",
            "example": "2024-04-24T09:24:23.186Z"
          },
          "dateDecision": {
            "type": "string",
            "description": "Date de la décision au format AAAA-MM-JJ",
            "example": "2024-04-23"
          },
          "decatt": {
            "description": "Décision attaquée : une décision de la Cour de cassation qui attaque une décision de cour d'appel",
