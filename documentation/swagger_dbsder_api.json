          "404": { "description": "La decision n'a pas été trouvée" }
        },
        "tags": ["DbSder"]
      }
    },
    "/v1/decisions/{id}/statut": {
      "put": {
        "operationId": "UpdateDecisionStatutController_updateDecisionStatut",
        "parameters": [
          {
            "name": "id",
            "required": true,
