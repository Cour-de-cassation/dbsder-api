{
  "openapi": "3.0.0",
  "paths": {
    "/v1/health": {
      "get": {
        "operationId": "HealthController_check",
        "parameters": [
          {
            "name": "x-api-key",
            "in": "header",
            "description": "Clé API",
            "schema": { "type": "string" }
          }
        ],
        "responses": {
          "200": {
            "description": "The Health Check is successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": { "type": "string", "example": "ok" },
                    "info": {
                      "type": "object",
                      "example": { "database": { "status": "up" } },
                      "additionalProperties": {
                        "type": "object",
                        "properties": { "status": { "type": "string" } },
                        "additionalProperties": { "type": "string" }
                      },
                      "nullable": true
                    },
                    "error": {
                      "type": "object",
                      "example": {},
                      "additionalProperties": {
                        "type": "object",
                        "properties": { "status": { "type": "string" } },
                        "additionalProperties": { "type": "string" }
                      },
                      "nullable": true
                    },
                    "details": {
                      "type": "object",
                      "example": { "database": { "status": "up" } },
                      "additionalProperties": {
                        "type": "object",
                        "properties": { "status": { "type": "string" } },
                        "additionalProperties": { "type": "string" }
                      }
                    }
                  }
                }
              }
            }
          },
          "503": {
            "description": "The Health Check is not successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": { "type": "string", "example": "error" },
                    "info": {
                      "type": "object",
                      "example": { "database": { "status": "up" } },
                      "additionalProperties": {
                        "type": "object",
                        "properties": { "status": { "type": "string" } },
                        "additionalProperties": { "type": "string" }
                      },
                      "nullable": true
                    },
                    "error": {
                      "type": "object",
                      "example": { "redis": { "status": "down", "message": "Could not connect" } },
                      "additionalProperties": {
                        "type": "object",
                        "properties": { "status": { "type": "string" } },
                        "additionalProperties": { "type": "string" }
                      },
                      "nullable": true
                    },
                    "details": {
                      "type": "object",
                      "example": {
                        "database": { "status": "up" },
                        "redis": { "status": "down", "message": "Could not connect" }
                      },
                      "additionalProperties": {
                        "type": "object",
                        "properties": { "status": { "type": "string" } },
                        "additionalProperties": { "type": "string" }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1/decisions/{id}": {
      "get": {
        "operationId": "GetDecisionByIdController_getDecisionById",
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
            "description": "Identifiant de la décision",
            "schema": { "type": "string" }
          },
          {
            "name": "x-api-key",
            "in": "header",
            "description": "Clé API",
            "schema": { "type": "string" }
          }
        ],
        "responses": {
          "200": {
            "description": "Décision trouvée",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/GetDecisionByIdResponse" }
              }
            }
          },
          "401": { "description": "Vous n'êtes pas autorisé à appeler cette route" },
          "404": { "description": "La decision n'a pas été trouvée" }
        },
        "tags": ["DbSder"]
      },
      "delete": {
        "operationId": "DeleteDecisionByIdController_deleteDecisionById",
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
            "description": "Identifiant de la décision",
            "schema": { "type": "string" }
          },
          {
            "name": "x-api-key",
            "in": "header",
            "description": "Clé API",
            "schema": { "type": "string" }
          }
        ],
        "responses": {
          "204": { "description": "La décision a bien été supprimée" },
          "401": { "description": "Vous n'êtes pas autorisé à appeler cette route" },
          "404": { "description": "La decision n'a pas été trouvée" },
          "422": { "description": "Entité non processable" },
          "503": { "description": "Service non disponible" }
        },
        "tags": ["DbSder"]
      }
    },
    "/v1/decisions-pseudonymisees/{id}": {
      "get": {
        "operationId": "GetDecisionPseudonymiseesController_getDecisionById",
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
            "description": "Identifiant de la décision",
            "schema": { "type": "string" }
          },
          {
            "name": "avecMetadonneesPersonnelles",
            "required": true,
            "in": "query",
            "schema": { "type": "boolean" }
          },
          {
            "name": "x-api-key",
            "in": "header",
            "description": "Clé API",
            "schema": { "type": "string" }
          }
        ],
        "responses": {
          "200": {
            "description": "La décision",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/GetDecisionByIdResponse" }
              }
            }
          },
          "401": { "description": "Vous n'êtes pas autorisé à appeler cette route" },
          "404": { "description": "La decision n'a pas été trouvée" }
        },
        "tags": ["DbSder"]
      }
    },
    "/v1/decisions": {
      "get": {
        "operationId": "ListDecisionsController_getDecisions",
        "parameters": [
          {
            "name": "source",
            "required": false,
            "in": "query",
            "description": "Source de la décision",
            "example": "juritj",
            "schema": { "type": "string" }
          },
          {
            "name": "status",
            "required": true,
            "in": "query",
            "description": "Décision intègre au format wordperfect et metadonnées associées.",
            "example": "toBeTreated",
            "schema": {
              "enum": [
                "toBeTreated",
                "loaded",
                "done",
                "exported",
                "blocked",
                "ignored_debatNonPublic",
                "ignored_decisionNonPublique",
                "ignored_codeDecisionBloqueCC",
                "ignored_dateDecisionIncoherente",
                "ignored_codeNACdeDecisionNonPublique",
                "ignored_codeNACdeDecisionPartiellementPublique",
                "ignored_codeNACInconnu",
                "ignored_caractereInconnu",
                "ignored_dateAvantMiseEnService",
                "ignored_controleRequis"
              ],
              "type": "string"
            }
          },
          {
            "name": "startDate",
            "required": true,
            "in": "query",
            "description": "date de début de la période de recherche",
            "example": "2024-04-24",
            "schema": { "type": "string" }
          },
          {
            "name": "endDate",
            "required": true,
            "in": "query",
            "description": "date de fin de la période de recherche",
            "example": "2024-04-24",
            "schema": { "type": "string" }
          },
          {
            "name": "numero",
            "required": false,
            "in": "query",
            "description": "NumeroRoleGeneral ou d'appel de la décision",
            "example": "01/12345",
            "schema": { "type": "string" }
          },
          {
            "name": "x-api-key",
            "in": "header",
            "description": "Clé API",
            "schema": { "type": "string" }
          }
        ],
        "responses": {
          "200": {
            "description": "Une liste de décisions",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": { "$ref": "#/components/schemas/GetDecisionsListResponse" }
                }
              }
            }
          },
          "400": {
            "description": "Le paramètre écrit n'est présent dans la liste des valeurs acceptées"
          },
          "401": { "description": "Vous n'êtes pas autorisé à appeler cette route" }
        },
        "tags": ["DbSder"]
      },
      "put": {
        "operationId": "CreateDecisionsController_createDecisions",
        "parameters": [
          {
            "name": "x-api-key",
            "in": "header",
            "description": "Clé API",
            "schema": { "type": "string" }
          }
        ],
        "requestBody": {
          "required": true,
          "description": "Décision intègre au format wordperfect et metadonnées associées.",
          "content": {
            "application/json": { "schema": { "$ref": "#/components/schemas/CreateDecisionDTO" } }
          }
        },
        "responses": {
          "200": {
            "description": "Décision créée ou mise à jour",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/CreateDecisionResponse" }
              }
            }
          },
          "400": {
            "description": "Il manque un ou plusieurs champs obligatoires dans la décision"
          },
          "401": { "description": "Vous n'êtes pas autorisé à appeler cette route" }
        },
        "tags": ["DbSder"]
      }
    },
