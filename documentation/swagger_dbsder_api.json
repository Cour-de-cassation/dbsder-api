            "type": "array",
            "items": { "type": "string" }
          },
          "jurisdictionCode": {
            "type": "string",
            "description": "Code de la juridiction",
            "example": "someJurisdictionCode"
          },
          "jurisdictionId": {
            "type": "string",
            "description": "Identifiant de la juridiction",
            "example": "someJurisdictionId"
          },
          "jurisdictionName": {
            "type": "string",
            "description": "Nom de la juridiction",
            "example": "someJurisdictionName"
          },
          "labelStatus": {
            "type": "string",
            "description": "Statut interne de la décision",
            "example": "toBeTreated"
          },
          "occultation": {
            "type": "object",
            "description": "Instruction d'occultation de la décision (champ non communiqué pour les décisions pseudonymisées)",
            "example": {
              "additionalTerms": "someAdditionalTerms",
              "categoriesToOmit": ["someCategoriesToOmit"]
            }
          },
          "originalText": {
            "type": "string",
            "description": "Texte original de la décision (champ non communiqué pour les décisions pseudonymisées)",
            "example": "someOriginalText"
          },
          "pseudoStatus": {
            "type": "string",
            "description": "Statut de la décision pseudonymisée",
