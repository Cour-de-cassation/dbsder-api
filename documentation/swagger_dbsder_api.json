        "required": [
          "_id",
          "appeals",
          "chamberId",
          "chamberName",
          "dateCreation",
          "dateDecision",
          "jurisdictionCode",
          "jurisdictionId",
          "jurisdictionName",
          "labelStatus",
          "originalText",
          "public",
          "registerNumber",
          "sourceId",
          "sourceName",
          "zoning",
          "blocOccultation"
        ]
      },
