            "schema": { "type": "string" }
          }
        ],
        "requestBody": {
          "required": true,
          "description": "Statut de la décision",
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/UpdateDecisionStatutDTO" }
            }
          }
        },
        "responses": {
          "204": { "description": "" },
