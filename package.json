{
  "name": "api-dbsder",
  "version": "0.0.1",
  "description": "API DBSDER",
  "author": "Cour de Cassation",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start:watch": "nodemon --watch 'src/**' --ext 'ts,json' --exec 'npx ts-node src/server.ts'",
    "start": "node dist/server.js",
    "build:watch": "tsc --watch",
    "build": "tsc --build",
    "format": "prettier \"src/**/*.ts\" --check",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "fix": "eslint \"src/**/*.ts\" --fix && prettier \"src/**/*.ts\" --write",
    "test": "jest --detectOpenHandles -c=jest.config.js"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "dbsder-api-types": "2.0.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "mongodb": "^6.15.0",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.31.0"
  }
}
